{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "lib": [
      "DOM",
      "es2019"
    ],
    "paths": {
      "*": [
        "node_modules/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "resolveJsonModule": true,
    "declaration": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "myst.ts",
    "tests/**/*", "src_archive/directives", "src_archive/plugins.ts", "src_archive/options.ts", "src_archive/state.ts", "src_archive/myst.ts", "src_archive/math", "src_archive/roles", "src_archive/index.ts", "src_archive/blocks.ts",
  ],
  "exclude": [
  ]
}
